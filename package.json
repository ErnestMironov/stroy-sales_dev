    "description": "",
    "main": "index.pug",
    "homepage": "https://ernestmironov.github.io/stroy-sales_dev/",
    "scripts": {
        "clean": "rimraf dist",
        "start": "parcel src/index.pug --open",
        "build-prod-github": "parcel build src/index.pug --public-url www/stroy-sales_dev/dist --no-content-hash",
        "build-prod": "parcel build src/index.pug  --no-content-hash"
    },
    "dependencies": {
        "autoprefixer": "^9.8.2",
